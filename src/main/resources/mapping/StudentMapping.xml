<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sun.testprojectforjob.mapper.StudentMapper">


    <select id="Sel" resultType="com.sun.testprojectforjob.entity.Student">
        select * from student where id = #{id}
    </select>

    <select id="getUser" resultType="com.sun.testprojectforjob.entity.User">
        select * from user where username = #{username}
    </select>
</mapper>